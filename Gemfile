# Gemfile

source "https://rubygems.org"

# Specify the modern Jekyll version for local development
gem "jekyll", "~> 4.3"

# Specify the Jekyll plugins your site uses
group :jekyll_plugins do
  gem "jekyll-feed", "~> 0.17.0"
  gem "jekyll-seo-tag", "~> 2.8.0"
  gem "jekyll-sitemap", "~> 1.4.0"
  gem "jekyll-include-cache", "~> 0.2.1"
end

# Required for Jekyll 4+ to have a local web server
gem "webrick", "~> 1.7"

gem "logger"
